{
  "manifest_version": 2,
  "name": "POST to lemmies",
  "version": "",
  "author": "Loys Caucheteux",
  "browser_specific_settings": {
    "gecko": {
      "id": "extension@postlemmies.com",
      "strict_min_version": "90.0"
    }
  },
  "description": "Cr√©er un Post dans lemmy en 3 clics",
  "homepage_url": "https://github.com/gummyWalrus/POST-to-lemmies",
  "icons": {
    "32": "icons/lemmy-32.png",
    "48": "icons/lemmy-48.png",
    "64": "icons/lemmy-64.png",
    "128": "icons/lemmy-128.png"
  },
  "background": {
    "scripts" : [
      "scripts/browser-polyfill.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "icons/lemmy-48.png",
    "default_title": "POST to lemmies",
    "default_popup": "popup/index.html"
  }
}

  
